{
  "name": "@nguniversal/common",
  "version": "16.2.0",
  "description": "Angular Universal common utilities",
  "license": "MIT",
  "keywords": [
    "ssr",
    "universal"
  ],
  "./tools": {
    "types": "./tools.d.ts",
    "default": "./tools/bundles/index.js"
  },
  "engines": {
    "node": "^16.14.0 || >=18.10.0"
  },
  "dependencies": {
    "critters": "0.0.20",
    "jsdom": "22.1.0",
    "tslib": "^2.3.0"
  },
  "peerDependencies": {
    "@angular/common": "^16.0.0 || ^16.1.0-next.0",
    "@angular/core": "^16.0.0 || ^16.1.0-next.0"
  },
  "schematics": "./schematics/collection.json",
  "ng-update": {
    "packageGroup": [
      "@nguniversal/builders",
      "@nguniversal/common",
      "@nguniversal/express-engine"
    ],
    "packageGroupName": "@nguniversal/common"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/angular/universal"
  },
  "bugs": {
    "url": "https://github.com/angular/universal/issues"
  },
  "homepage": "https://github.com/angular/universal",
  "module": "./fesm2022/common.mjs",
  "typings": "./index.d.ts",
  "type": "module",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./index.d.ts",
      "esm2022": "./esm2022/common.mjs",
      "esm": "./esm2022/common.mjs",
      "default": "./fesm2022/common.mjs"
    },
    "./clover": {
      "types": "./clover/index.d.ts",
      "esm2022": "./esm2022/clover/clover.mjs",
      "esm": "./esm2022/clover/clover.mjs",
      "default": "./fesm2022/clover.mjs"
    },
    "./clover/server": {
      "types": "./clover/server/index.d.ts",
      "esm2022": "./esm2022/clover/server/server.mjs",
      "esm": "./esm2022/clover/server/server.mjs",
      "default": "./fesm2022/clover/server.mjs"
    },
    "./engine": {
      "types": "./engine/index.d.ts",
      "esm2022": "./esm2022/engine/engine.mjs",
      "esm": "./esm2022/engine/engine.mjs",
      "default": "./fesm2022/engine.mjs"
    },
    "./tools": {
      "types": "./tools/index.d.ts",
      "esm2022": "./esm2022/tools/tools.mjs",
      "esm": "./esm2022/tools/tools.mjs",
      "default": "./fesm2022/tools.mjs"
    }
  }
}