{
  "name": "@nguniversal/express-engine",
  "version": "16.2.0",
  "description": "Express Engine for running Server Angular Apps",
  "license": "MIT",
  "keywords": [
    "express",
    "ssr",
    "universal"
  ],
  "engines": {
    "node": "^16.14.0 || >=18.10.0"
  },
  "peerDependencies": {
    "@angular/common": "^16.0.0 || ^16.1.0-next.0",
    "@angular/core": "^16.0.0 || ^16.1.0-next.0",
    "express": "^4.15.2"
  },
  "dependencies": {
    "@nguniversal/common": "16.2.0",
    "tslib": "^2.3.0"
  },
  "schematics": "./schematics/collection.json",
  "ng-update": {
    "packageGroup": [
      "@nguniversal/builders",
      "@nguniversal/common",
      "@nguniversal/express-engine"
    ],
    "packageGroupName": "@nguniversal/express-engine"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/angular/universal"
  },
  "bugs": {
    "url": "https://github.com/angular/universal/issues"
  },
  "homepage": "https://github.com/angular/universal",
  "module": "./fesm2022/express-engine.mjs",
  "typings": "./index.d.ts",
  "type": "module",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./index.d.ts",
      "esm2022": "./esm2022/express-engine.mjs",
      "esm": "./esm2022/express-engine.mjs",
      "default": "./fesm2022/express-engine.mjs"
    },
    "./tokens": {
      "types": "./tokens/index.d.ts",
      "esm2022": "./esm2022/tokens/tokens.mjs",
      "esm": "./esm2022/tokens/tokens.mjs",
      "default": "./fesm2022/tokens.mjs"
    }
  }
}